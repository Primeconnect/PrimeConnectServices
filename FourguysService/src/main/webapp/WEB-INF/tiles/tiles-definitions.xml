<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
  
    <definition name="defaultTemplate" template="/WEB-INF/template/default/template.jsp">
        <put-attribute name="header" value="/WEB-INF/template/default/header.jsp" />
        <put-attribute name="menu" value="/WEB-INF/template/default/menu.jsp" />
        <put-attribute name="footer" value="/WEB-INF/template/default/footer.jsp" />
    </definition>

    <definition name="root-container" template="/WEB-INF/fgs/root-container.jsp">
        <put-attribute name="header" value="/WEB-INF/fgs/header.jsp" />
        <put-attribute name="main-menu" value="/WEB-INF/fgs/main-menu.jsp" />
        <put-attribute name="footer" value="/WEB-INF/fgs/footer.jsp" />
    </definition>

    <definition name="fgs.login" template="/WEB-INF/fgs/root-container.jsp">
        <put-attribute name="header" value="/WEB-INF/fgs/header.jsp" />
        <put-attribute name="main-menu" value="/WEB-INF/fgs/main-menu.jsp" />
        <put-attribute name="content">
        	<definition template="/WEB-INF/fgs/login/fgs-login-container.jsp">
        		<put-attribute name="fgs.login.google" value="/WEB-INF/fgs/login/google-login.jsp"/>
    			<put-attribute name="fgs.login.fb" value="/WEB-INF/fgs/login/fb-login.jsp"/>
    			<put-attribute name="fgs.login" value="/WEB-INF/fgs/login/fgs-login.jsp"/>
        	</definition>
        </put-attribute>
        <put-attribute name="footer" value="/WEB-INF/fgs/footer.jsp" />
    </definition>
</tiles-definitions>